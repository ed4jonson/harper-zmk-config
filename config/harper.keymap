&lt { quick_tap_ms = <200>; };

&mt { quick_tap_ms = <200>; };

/ {
    keymap {
        compatible = "zmk,keymap";

        default_layer {
            bindings = <
&mt LEFT_META ESCAPE  &kp N1  &kp N2     &kp N3    &kp N4    &kp N5              &kp N6        &kp N7      &kp N8      &kp N9         &kp N0  &kp BACKSPACE
&mt LEFT_META ESCAPE  &kp Q   &kp W      &kp E     &kp R     &kp T               &kp Y         &kp U       &kp I       &kp O          &kp P   &kp BACKSPACE
&lt 1 TAB             &kp A   &kp S      &kp D     &kp F     &kp G               &kp SQT       &kp H       &kp J       &kp K          &kp L   &kp ENTER
&kp LSHFT             &kp Z   &kp X      &kp C     &kp V     &kp COMMA           &kp C_MUTE    &kp DELETE  &kp PERIOD  &kp SEMICOLON  &kp B   &kp N
&kp M                 &mo 1   &kp LCTRL  &kp LGUI  &kp LALT  &mo 4      &mo RSE  &kp LEFT_ALT  &kp SPACE   &kp SPACE   &kp RIGHT_ALT  &mo 2   &mo 4          &mo 3
                                                                                 &mo 3                                                                       &kp RCTRL
            >;

            sensor-bindings =
                <&inc_dec_kp K_SCROLL_DOWN K_SCROLL_UP>,
                <&inc_dec_kp LEFT RIGHT>;
        };

        navigation_layer {
            bindings = <
&kp ESC  &kp F1       &kp F2        &kp F3         &kp F4         &kp F5          &kp F6  &kp F7     &kp F8  &kp F9  &kp F10  &kp F11
&trans   &kp PAGE_UP  &kp UP_ARROW  &kp PAGE_DOWN  &none          &none           &trans  &trans     &trans  &trans  &trans   &trans
&trans   &kp LEFT     &kp DOWN      &kp RIGHT      &none          &none           &trans  &trans     &trans  &trans  &trans   &trans
&trans   &kp HOME     &kp DELETE    &kp END        &kp BACKSPACE  &none           &trans  &none      &trans  &trans  &trans   &trans
&trans   &trans       &trans        &trans         &trans         &trans  &trans  &trans  &kp ENTER  &trans  &trans  &trans   &trans   &trans
                                                                                  &trans                                               &trans
            >;

            sensor-bindings =
                <&inc_dec_kp C_VOL_DN C_VOL_UP>,
                <&inc_dec_kp DOWN UP_ARROW>;
        };

        number_layer {
            bindings = <
&none      &none         &none         &none         &none       &none                     &none         &none       &none      &none        &none             &none
&none      &kp N7        &kp N8        &kp N9        &kp N0      &kp QUESTION              &kp GRAVE     &kp DOLLAR  &kp UP     &kp END      &none             &none
&none      &kp NUMBER_4  &kp N5        &kp NUMBER_6  &kp SLASH   &kp EXCLAMATION           &kp HASH      &kp AT      &kp DOWN   &kp RIGHT    &kp DEL           &kp BSPC
&trans     &kp NUMBER_1  &kp NUMBER_2  &kp NUMBER_3  &kp C_MUTE  &none                     &none         &none       &none      &kp PERCENT  &kp LEFT_BRACKET  &kp RBKT
&kp SLASH  &none         &trans        &kp LGUI      &kp LALT    &mo ADJ          &mo RSE  &mo LWR       &kp SPACE   &kp SPACE  &mo RSE      &kp PERIOD        &kp EQUAL  &kp PLUS
                                                                                           &kp ASTERISK                                                                   &kp MINUS
            >;

            sensor-bindings = <&inc_dec_kp C_VOL_DN C_VOL_UP &inc_dec_kp PG_DN PG_UP>;
